    <div class="container-fluid">
      <div class="row">
        <div class="col-xl-5" style="background-color: rgba(99,98,231,0.1);"><img class="bg-img-cover bg-center" src="/theme/assets/images/login/loginRealFevr.png" alt="loginpage"></div>
        <div class="col-xl-7 p-0">
          <div class="login-card">
            <form class="theme-form login-form needs-validation" novalidate="">
              <h4>Login</h4>
              <h6>Welcome back! Log in to your account.</h6>
              <div class="form-group">
                <label>Email Address</label>
                <div class="input-group"><span class="input-group-text"><i class="icon-email"></i></span>
                  <input class="form-control username" type="email" required="" placeholder="user@email.com">
                  <div class="invalid-tooltip">Please enter proper email.</div>
                </div>
              </div>
              <div class="form-group">
                <label>Password</label>
                <div class="input-group"><span class="input-group-text"><i class="icon-lock"></i></span>
                  <input class="form-control password" type="password" name="login[password]" required="" placeholder="*********">
                  <div class="invalid-tooltip">Please enter password.</div>
                  <div class="show-hide"><span class="show">                         </span></div>
                </div>
              </div>
              <div class="form-group">
                <div class="checkbox">
                  <input id="checkbox1" class="rememberme" type="checkbox">
                  <label class="text-muted" for="checkbox1">Remember password                    </label>
                </div>
              </div>
              <div class="form-group">
                <button class="btn btn-primary btn-block" type="submit">Sign in        </button>
              </div>
            </form>
          </div>
        </div>
      </div>

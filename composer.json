{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "barbieswimcrew/zip-code-validator": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "friendsofsymfony/rest-bundle": "^2.7",
        "friendsofsymfony/user-bundle": "2.x-dev",
        "imagecow/imagecow": "^2.4",
        "jms/serializer-bundle": "^3.5",
        "league/flysystem-bundle": "^1.5",
        "nelmio/api-doc-bundle": "^3.6",
        "omines/datatables-bundle": "^0.5.2",
        "phpdocumentor/reflection-docblock": "^5.1",
        "rollerworks/password-strength-bundle": "^2.2",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/debug": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "twig/twig": "^3.0",
        "white-october/pagerfanta-bundle": "^1.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "fzaninotto/faker": "^1.9",
        "ottaviano/faker-gravatar": "^0.1.2",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/phpunit-bridge": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "exclude-from-classmap": ["vendor/symfony/http-foundation/ParameterBag.php"],
        "psr-4": {
            "App\\": "src/"            
        },
        "files": ["Fixes/ParameterBag.php"]       
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
